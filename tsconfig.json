{
	"moduleResolution": "node",
	"compilerOptions": {
		"lib": ["es2020"],
		"module": "commonjs",
		"target": "es2020",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"importHelpers": true,
		"removeComments": true
	}
}
